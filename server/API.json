{
  "requests": {

    "Authorization": {

      "request": {
        "header": "auth",
        "body": {
          "name": ":string:"
        }
      },

      "response": {
        "header": "response",
        "body": {
          "type": {
            "varType": ":string:",
            "varEnum": [
              "authSucc",
              "authFail"
            ]
          },
          "message": ":string:",
          "error": ":bool:"
        }
      }

    },

    "GetRooms": {
      "request": {
        "header": "getRooms",
        "body": {
          "room": {
            "varTypes": [
              ":string:",
              ":null:"
            ]
            }
        },
        "desctiption": "Send string to get rooms by name. Send null to get all rooms."
      },

      "response": {
        "header": "response",
        "body": {
          "type": {
            "varType": ":string:",
            "varEnum": "rooms"
          },
          "message": ":Array(:string:):",
          "error": ":bool:"
        }
      }

    }

  }
}