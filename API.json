{
  "Calls": {

    "Authorization": {

      "header": "auth",
      "request": {
        "body": {
          "name": ":string:",
          "password": ":string:"
        }
      },

      "response": {
        "body": {
          "data": {
            "successful": ":string:",
            "unsuccessful": [
              "badName",
              "badPassword",
              "wrongNamePass",
              "alreadyInSess"
            ],
            "error": [
              "invalidData"
            ]
          },
          "successful": true,
          "error": true
        }
      }
    },

    "DeAuthorization": {

      "header": "deAuth",
      "request": {
        "body": {
        }
      },

      "response": {
        "body": {
          "data": {
            "successful": ":string:",
            "unsuccessful": [
              "notAuth"
            ],
            "error": []
          },
          "successful": true,
          "error": true
        }
      }
    },

    "ChangeProfile": {

      "header": "changeProfile",
      "request": {
        "body": {
          "!name": ":string:",
          "!avatar": ":base64string:"
        }
      },

      "response": {
        "body": {
          "data": {
            "successful": ":string:",
            "unsuccessful": [
              "notAuth",
              "badName",
              "badImage",
              "nameExists"
            ],
            "error": [
              "invalidData"
            ]
          },
          "successful": true,
          "error": true
        }
      }
    },

    "GetUsers": {

      "header": "getUsers",
      "request": {
        "body": {
          "Users": [
            [
              ":uint:"
            ],
            null
          ],
          "Select": [
            "name",
            "avatar",
            "avatarHash"
          ]
        }
      },

      "response": {
        "body": {
          "data": {
            "successful": {
              "data": [
                {
                  "id": ":uint:",
                  "!name": ":string:",
                  "!avatar": ":base64string:",
                  "!avatarHash": ":string:"
                }
              ]
            },
            "unsuccessful": [
              "notAuth"
            ],
            "error": []
          },
          "successful": true,
          "error": true
        }
      }
    },

    "Registration": {

      "header": "registration",
      "request": {
        "body": {
          "name": ":string:",
          "password": ":string:"
        }
      },

      "response": {
        "body": {
          "data": {
            "successful": ":string:",
            "unsuccessful": [
              "badName",
              "badPassword",
              "nameExists"
            ],
            "error": [
              "invalidData",
              "uknownError"
            ]
          },
          "successful": true,
          "error": true
        }
      }
    },

    "GetRooms": {

      "header": "getRooms",

      "request": {
        "body": {
          "room": [
            ":string:",
            null
          ]
        },
        "desctiption": "Send string to get rooms by name. Send null to get all rooms."
      },

      "response": {
        "body": {
          "data": [
            {
              "id": ":int:",
              "room": ":string:",
              "haspass": true
            }
          ],
          "successful": true,
          "error": true
        },
        "desctiption": "Returns array sorted by input string like."
      }

    },

    "Chat": {

      "header": "chat",
      "request": {
        "body": {
          "message": ":string:"
        }
      },

      "response": {
        "body": {
          "userid": ":uint:",
          "roomid": ":int:",
          "message": ":string:"
        }
      }
    },

    "JoinRoom": {

      "header": "joinRoom",
      "request": {
        "body": {
          "id": ":int:",
          "password": [
            ":string:",
            null
          ]
        }
      },

      "response": {
        "body": {
          "data": {
            "successful": {
              "id": ":int:",
              "room": ":string:",
              "message": ":string:"
            },
            "unsuccessful": [
              "invalidPass",
              "roomNotFound"
            ],
            "error": [
              "invalidData"
            ]
          },
          "successful": true,
          "error": true
        }
      }
    },


    "LeaveRoom": {

      "header": "leaveRoom",
      "request": {
        "body": {
          "id": ":int:"
        }
      },

      "response": {
        "body": {
          "data": {
            "successful": {
              "id": ":int:",
              "room": ":string:",
              "message": ":string:"
            },
            "unsuccessful": [
              "roomNotFound",
              "notInRoom"
            ],
            "error": [
              "invalidData"
            ]
          },
          "successful": true,
          "error": true
        }
      }
    },


    "CreateRoom": {

      "header": "createRoom",
      "request": {
        "body": {
          "name": ":string:",
          "password": [
            ":string:",
            null
          ]
        }
      },

      "response": {
        "body": {
          "data": {
            "successful": {
              "id": ":int:",
              "name": ":string:",
              "message": ":string:"
            },
            "unsuccessful": [
              "roomExists",
              "badName"
            ],
            "error": [
              "invalidData"
            ]
          },
          "successful": true,
          "error": true
        }
      }
    }

  },

  "ServerCalls": {

    "Access": {
      "header": "access",
      "response": {
        "body": {
          "data": {
            "successful": ":string:",
            "error": [
              "notAuth"
            ]
          },
          "successful": true,
          "error": true
        }
      }
    },

    "RoomChange": {
      "header": "roomChange",
      "response": {
        "body": {
          "data": ":string:",
          "successful": true,
          "error": true
        }
      }

    }

  }

}